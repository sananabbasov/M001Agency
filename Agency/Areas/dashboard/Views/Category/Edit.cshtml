@model Category



<form enctype="multipart/form-data" method="post">
    <div class="container">
        <div class="row">
            <div class="input-group my-2">
                <input value="@Model.Name" name="Name" id="title" class="form-control" type="text" placeholder="Name" />
            </div>
           
            <input type="submit" class="btn btn-outline-warning" value="Update" />
        </div>
    </div>  
</form>